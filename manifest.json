{
  "manifest_version": 2,

  "name": "Crrection!",
  "description": "Make corrections to a webpage and send them in.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"],
      "css": ["style.css"]
    }
  ],

  "permissions": [
    "activeTab"
  ]
}
